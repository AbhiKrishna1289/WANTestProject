package com.volume;
import java.util.Scanner;

public class Volume {
	//Finding volume of Sphere
	public static double sphere(double r)
	{
		double PI=3.14;
		double volume=(float)4/(float)3*PI*r*r*r;
		return(volume);

	}
	//Finding volume of Cube
	public double cube()
